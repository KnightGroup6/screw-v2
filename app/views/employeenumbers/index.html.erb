<h1>Listing employeenumbers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employeenumbers.each do |employeenumber| %>
      <tr>
        <td><%= employeenumber.name %></td>
        <td><%= link_to 'Show', employeenumber %></td>
        <td><%= link_to 'Edit', edit_employeenumber_path(employeenumber) %></td>
        <td><%= link_to 'Destroy', employeenumber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employeenumber', new_employeenumber_path %>
