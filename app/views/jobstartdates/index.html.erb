<h1>Listing jobstartdates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobstartdates.each do |jobstartdate| %>
      <tr>
        <td><%= jobstartdate.name %></td>
        <td><%= link_to 'Show', jobstartdate %></td>
        <td><%= link_to 'Edit', edit_jobstartdate_path(jobstartdate) %></td>
        <td><%= link_to 'Destroy', jobstartdate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobstartdate', new_jobstartdate_path %>
